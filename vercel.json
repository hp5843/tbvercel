{
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.4.0"
    }
  },
  "routes": [
    {
      "src": "/(static|admin)/(.*)",
      "dest": "public/$1/$2"
    },
    {
      "src": "/favicon.ico",
      "dest": "public/favicon.ico"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ]
}